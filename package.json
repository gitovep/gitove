{
  "name": "gitove",
  "version": "0.0.1",
  "type": "module",
  "description": "An interactive git cli tool for consistant commit messages.",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc",
    "start": "pnpm build && node --loader=./dist/config/loader.js --experimental-specifier-resolution=node --no-warnings dist/index.js",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "postinstall": "ts-patch install"
  },
  "bin": {
    "gitove": "dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "execa": "5.1.1",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "esm-module-alias": "^2.0.3",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.1.6",
    "typia": "^4.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  }
}